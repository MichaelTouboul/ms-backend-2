{
  "name": "@ms/sls-auth",
  "version": "1.0.0",
  "private": true,
  "main": "dist/lambda.js",
  "scripts": {
    "predev": "npm -w @ms/auth run build",
    "dev": "nodemon",
    "dev:once": "dotenv -e .env.local -- serverless offline --stage dev",
    "deploy:prod": "dotenv -e .env.prod -- serverless deploy --stage prod",
    "deploy:dev": "serverless deploy --stage dev"
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "^3.968.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/mongoose": "^11.0.4",
    "@nestjs/platform-express": "^11.1.11",
    "@vendia/serverless-express": "^4.12.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "jose": "^5.0.0",
    "mongoose": "^9.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.159",
    "@types/node": "^25.0.7",
    "dotenv-cli": "^11.0.0",
    "nodemon": "^3.1.11",
    "serverless": "^4.30.0",
    "serverless-esbuild": "^1.57.0",
    "serverless-offline": "^14.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
